#include <stdio.h>

void Horizontal (int digit[100])
{
    
    for (int i=0; i<100; i++)
    {
        if (digit[i] != 0)
        {
            putchar(i+'0');
            printf("-");
            for (int j=0; j < digit[i]; j++)
            {
                putchar('#');
            }
            putchar('\n');
        }
    }
}



int main() {
    int c;
    int digit[100] = {0};

    for(int count = 0; c != '\n'; count++){
        c = getchar();

        if (c >= '0' && c <= '9')
            ++digit[c-'0'];
    }
    Horizontal(digit);
    
}
