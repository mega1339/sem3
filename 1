#include <stdio.h>
#include <locale.h>




int main() {
    
    setlocale(LC_ALL, ".1251");
    char  ch, i;
    int count = 0;
    char mask[4], c;
    puts("enter:");
    while ((ch = getchar()) != '\n' && ch != EOF) {
        if (count < 2) {
            mask[count] = ch;
            count += 1;
        } else {
            if (count == 2) {
                mask[2] = ch;
            }
            if (mask[0] != '-' && mask[1] == '-' && mask[2] != '-') {
                if (mask[0] > mask[2]) {
                    if (c != mask[0])
                        putchar(mask[0]);
                    for (i = mask[0] - 1; i >= mask[2]; i--)
                        putchar(i);
                    c = i - 1;
                } else {
                    if (c != mask[0])
                        putchar(mask[0]);
                    for (i = mask[0] + 1; i <= mask[2]; i++)
                        putchar(i);
                    c = i - 1;
                }
            }
            for (i = 0; i < 3; i++)
                mask[i] = mask[i + 1];
            mask[2] = ch;
        }
    }
    return 0;
}
